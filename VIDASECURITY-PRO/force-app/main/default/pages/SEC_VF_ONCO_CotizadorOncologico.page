                                        '</tem:ValidacionUsuarioPorFecNac>' +
                                    '</soapenv:Body>' +
                                '</soapenv:Envelope>';
                            
                                //alert(sr);
                                                              
                                xmlhttp.open('POST', 'https://servicios.vidasecurity.cl/wsProductoOnco/wsProductoOncologico.asmx?wsdl', true);
